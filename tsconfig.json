{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "lib": ["dom", "es5", "es6"],
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true
  }
}
