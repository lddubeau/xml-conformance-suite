{
  "name": "xml-conformance-suite",
  "version": "1.2.0",
  "description": "A conformance suite for tools that parse XML.",
  "scripts": {
    "test": "mocha",
    "posttest": "npm run lint",
    "lint": "eslint *.js bin/* \"js/**/*.js\" \"test/**/*.js\"",
    "preversion": "npm run test",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "npm publish",
    "postpublish": "git push origin --follow-tags",
    "watch": "mocha --watch"
  },
  "keywords": [
    "XML",
    "parsing",
    "parser",
    "test"
  ],
  "author": "Louis-Dominique Dubeau <ldd@lddubeau.com>",
  "license": "MIT AND W3C-19980720",
  "repository": {
    "type": "git",
    "url": "https://github.com/lddubeau/xml-conformance-suite.git"
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "chai": "^4.2.0",
    "minimist": "^1.2.0",
    "mocha": "^6.2.2",
    "saxes": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "conventional-changelog-cli": "^2.0.28",
    "eslint": "^6.6.0",
    "eslint-config-lddubeau-base": "^4.0.2",
    "husky": "^3.1.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "sax": "^1.2.4",
    "serve-static": "^1.14.1",
    "systemjs": "^0.21.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
