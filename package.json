{
  "name": "xml-conformance-suite",
  "version": "1.0.0",
  "description": "A conformance suite for tools that parse XML.",
  "scripts": {
    "test": "mocha && npm run lint",
    "lint": "eslint *.js bin/* \"js/**/*.js\" \"test/**/*.js\"",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "watch": "mocha --watch"
  },
  "keywords": [
    "XML",
    "parsing",
    "parser",
    "test"
  ],
