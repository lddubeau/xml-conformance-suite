  "license": "MIT AND W3C-19980720",
  "repository": {
    "type": "git",
    "url": "https://github.com/lddubeau/xml-conformance-suite.git"
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-lddubeau-base": "^2.2.0",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "posix-pipe": "^0.1.4",
    "saxen": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "husky": "^0.14.3",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "sax": "^1.2.4",
    "serve-static": "^1.13.2",
    "systemjs": "^0.21.4"
  }
